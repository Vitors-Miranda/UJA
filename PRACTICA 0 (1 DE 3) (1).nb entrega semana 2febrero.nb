 "\nF\[OAcute]rmula de Simpson compuesta:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", "a", "b"], 
     RowBox[{
      RowBox[{"f", "(", "x", ")"}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], " ", "\[TildeEqual]", 
    RowBox[{
     FractionBox["h", "6"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"f", "[", 
        SubscriptBox["x", "1"], "]"}], "+", 
       RowBox[{"f", "[", 
        SubscriptBox["x", 
         RowBox[{"n", "+", "1"}]], "]"}], "+", 
       RowBox[{"2", 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"i", "=", "2"}], "n"], 
         RowBox[{"f", "[", 
          SubscriptBox["x", "i"], "]"}]}]}], "+", 
       RowBox[{"4", 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"i", "=", "1"}], "n"], 
         RowBox[{"f", "[", 
          RowBox[{
           SubscriptBox["x", "i"], "+", 
           FractionBox["h", "2"]}], "]"}]}]}]}], ")"}]}]}], TraditionalForm]],
  TextAlignment->Center,ExpressionUUID->
  "58cc274e-ac0b-49a4-81ed-79bc10f0bdb3"],
 "\ndonde ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"h", "=", 
      FractionBox[
       RowBox[{"b", "-", "a"}], "n"]}], ";", " ", 
     RowBox[{
      SubscriptBox["x", "1"], "=", "a"}]}], ",", " ", 
    RowBox[{
     SubscriptBox["x", 
      RowBox[{"n", "+", "1"}]], "=", "b"}]}], TraditionalForm]],
  ExpressionUUID->"d23993d3-eafa-46df-a1b4-a5895bfff69f"]
}], "Text",
