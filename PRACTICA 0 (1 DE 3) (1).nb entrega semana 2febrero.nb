Cell[TextData[{
 "La cota del error cometido al utilizar la regla compuesta de Simpson para \
calcular ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", "a", "b"], 
    RowBox[{
     RowBox[{"f", "(", "x", ")"}], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "08e7f41e-1bd0-4315-a9da-4e51fa109a91"],
 " es:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
    "Cota", " ", "del", " ", "error", " ", "de", " ", "Simpson", " ", 
     "compuesta"}], " ", "=", " ", 
    RowBox[{
     UnderscriptBox["M\[AAcute]x", 
      RowBox[{"x", "\[Element]", 
       RowBox[{"[", 
        RowBox[{"a", ",", "b"}], "]"}]}]], 
     RowBox[{"\[LeftBracketingBar]", 
      RowBox[{
       SuperscriptBox["f", 
        RowBox[{"(", "4", ")"}]], "[", "x", "]"}], "\[RightBracketingBar]"}], 
     FractionBox[
      SuperscriptBox["h", "4"], "2880"], 
     RowBox[{"(", 
      RowBox[{"b", "-", "a"}], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "0a3ff6d8-a2c2-4ea0-b18b-d7e98b4e9e4e"]
}], "Text",
